
@model IEnumerable<InventoryTask.Models.ListOfPurchase>
@{
    ViewBag.Title = "ListOfPurchaseOrderAgainstCustomer";
}

<h2>List Of PurchaseOrder Against Customer</h2>

<table id="1" class="table">
    <tr>
        <th>Customer Name</th>
        <th>PurchaseOrder ID</th>
        <th>Product Name</th>
        <th>Price</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.CustomerName.Name</td>
            <td>@item.POID.POID</td>
            <td>@item.ProductName.Name</td>
            <td>@item.Price.Price</td>
        </tr>
    }
</table>